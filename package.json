{
  "name": "distype",
  "version": "2.0.0",
  "description": "A Discord library written in TypeScript for developers looking to interface directly with the Discord API with little to no abstractions.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "docs": "rimraf ./docs && typedoc",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "npm i --include=dev && npm run build"
  },
  "keywords": [
    "api",
    "bot",
    "client",
    "discord",
    "discord-api",
    "discord-bot",
    "discordapp",
    "distype"
  ],
  "homepage": "https://distype.github.io/distype",
  "bugs": {
    "url": "https://github.com/distype/distype/issues"
  },
  "license": "Apache-2.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distype/distype.git"
  },
  "dependencies": {
    "@br88c/extended-map": "^1.0.0",
    "@br88c/typed-emitter": "^1.0.0",
    "@types/ws": "^8.5.4",
    "discord-api-types": "0.37.43",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.6.0",
    "@types/node": "^18.16.16",
    "eslint": "^8.41.0",
    "rimraf": "^5.0.1",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.14.0",
    "npm": ">=9.3.1"
  },
  "private": false
}
